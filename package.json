{
  "name": "tsc-progress",
  "version": "0.5.0",
  "description": "a starter for typescript lib",
  "keywords": [
    "typescript",
    "lib",
    "template",
    "aiou"
  ],
  "license": "MIT",
  "homepage": "https://github.com/spring-catponents/ts-lib-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spring-catponents/ts-lib-template.git"
  },
  "bugs": {
    "url": "https://github.com/spring-catponents/ts-lib-template/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && ttsc",
    "run_dev": "rimraf lib && ttsc",
    "dev": "npm-watch run_dev",
    "test": "pnpm run build && jest",
    "pretest": "pnpm run build",
    "ci:publish": "pnpm run build && pnpx changeset publish",
    "ci:version": "pnpx changeset version",
    "ci:snapshot": "pnpx changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpx changeset publish --tag beta",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "watch": {
    "run_dev": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": true,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": false
    }
  },
  "dependencies": {
    "ansi-escapes": "4.x",
    "chalk": "4.x",
    "figures": "3.x",
    "markdown-table": "^3.0.2",
    "picocolors": "^1.0.0",
    "pretty-time": "^1.1.0",
    "webpack": "^5.65.0",
    "webpackbar": "^5.0.2",
    "wrap-ansi": "7.x"
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.2.1",
    "@changesets/cli": "^2.16.0",
    "@types/cli-progress": "^3.9.2",
    "@types/jest": "26.0.23",
    "@types/node": "^17.0.4",
    "@types/pretty-time": "^1.1.2",
    "@types/wrap-ansi": "^8.0.1",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.30.0",
    "esrua": "^0.0.2",
    "husky": "^7.0.0",
    "jest": "27.0.6",
    "lint-staged": "^11.0.1",
    "np": "7.5.0",
    "npm-watch": "0.10.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "tslib": "2.3.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5",
    "typescript-transform-extensions": "^1.0.1",
    "typescript-transform-paths": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
